<Patch>	

	<Operation Class="AttributeMath">
		<xpath>/data/Element/me/data/l/element/features/engineHub[@massCapacity]</xpath>
		<attribute>massCapacity</attribute>
		<value opType="multiply">{massCapacity}</value>
	</Operation>

	<Operation Class="AttributeMath">
		<xpath>/data/Element/me/data/l/element/features/engineHub[@fuelBoost]</xpath>
		<attribute>fuelBoost</attribute>
		<value opType="multiply">{fuelBoost}</value>
	</Operation>

	<Operation Class="AttributeMath">
		<xpath>/data/Element/me/data/l/element/features/engineHub/fuel[@howMuch]</xpath>
		<attribute>howMuch</attribute>
		<value opType="multiply">{howMuch}</value>
	</Operation>

	<Operation Class="AttributeMath">
		<xpath>/data/Element/me/data/l/element/features/engineHub/fuel[@consumeEvery]</xpath>
		<attribute>consumeEvery</attribute>
		<value opType="multiply">{consumeEvery}</value>
	</Operation>

</Patch>