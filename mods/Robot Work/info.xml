<mod>
	<name>Robot Work</name>
	<author>WP</author>

	<gameVersions>
		<v>0.12.0</v>
	</gameVersions>

	<minimumLoaderVersion>0.9.2</minimumLoaderVersion>

	<version>21.06.01</version>

	<description>
Adjust which tasks robots can perform.

Only enter "true" or "false".
Clear quicklaunch file after changing values.
	</description>

	<config>
		<var name="{2874 logistics}" default="true">Logistic Bot: Logistics</var>
		<var name="{2874 dismantle}" default="false">Logistic Bot: Dismantle</var>
		<var name="{2874 repair}" default="false">Logistic Bot: Repair</var>
		<var name="{2874 build}" default="false">Logistic Bot: Build</var>

		<var name="{2888 logistics}" default="true">Salvage Bot: Logistics</var>
		<var name="{2888 dismantle}" default="true">Salvage Bot: Dismantle</var>
		<var name="{2888 repair}" default="false">Salvage Bot: Repair</var>
		<var name="{2888 build}" default="false">Salvage Bot: Build</var>

	</config>

</mod>